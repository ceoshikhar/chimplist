{
  "name": "chimplist",
  "version": "1.0.0",
  "description": "Mailchimp's API v3 wrapper for Lists/Audiences.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon lib/index.ts",
    "build": "tsc -p .",
    "test": "npm run build && node dist/test.js",
    "format": "prettier --config .prettierrc 'lib/**/*.ts'  --write",
    "lint": "eslint  --ext .ts . ",
    "fix": "eslint  --fix --ext .ts ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceoshikhar/chimplist.git"
  },
  "keywords": [
    "mailchimp",
    "mailchimp-api",
    "mailchimp-lists",
    "mailchimp-audiences",
    "api",
    "lists",
    "audiences",
    "api-v3"
  ],
  "author": "Shikhar Sharma <ceoshikhar@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ceoshikhar/chimplist/issues"
  },
  "homepage": "https://github.com/ceoshikhar/chimplist#readme",
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0"
  }
}
